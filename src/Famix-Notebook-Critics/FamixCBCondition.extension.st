Extension { #name : 'FamixCBCondition' }

{ #category : '*Famix-Notebook-Critics' }
FamixCBCondition >> rulesId [

	<FMProperty: #rulesId type: #String>
	<FMComment: 'Unique id for each conditions'>
	^rulesId.
]

{ #category : '*Famix-Notebook-Critics' }
FamixCBCondition >> rulesId: anObject [

	rulesId := anObject
]
